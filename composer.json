{
    "name": "moay/php-fints-database",
    "description": "Provides a database of german banks with fints settings",
    "license": "MIT",
    "authors": [
        {
            "name": "moay",
            "email": "mv@moay.de"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "moay/fints-db",
                "version": "master",
                "dist": {
                    "type": "file",
                    "url": "https://github.com/moay/fints-institute-db/raw/master/fints-institutes.json",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "moay/fints-db": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "moay\\FintsDatabase\\": "src",
            "moay\\FintsDatabase\\Test\\": "tests"
        }
    }
}
